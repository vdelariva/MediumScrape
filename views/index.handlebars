<div id="wrapper">
  <div id="articles">
    {{#each articles}}
      <h3 data-id={{this._id}}>{{this.headline}}</h3>
      <p>{{this.link}}</p>
      <p>Summary: {{this.summary}}</p>
      <p>Byline: {{this.author}}</p>
      <p>{{this.readTime}}</p>
      <p>{{this.dateAdded}}</p>
    {{/each}}
  </div>
  <div id="notes"></div>
</div>
